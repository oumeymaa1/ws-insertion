PROCEDURE insertion_sort(VAR arr:ARRAY_OF_INTEGER)
VAR
i,j,current: INTEGER;

BEGIN
    FOR i FROM 1 TO arr.length-1  DO
        current :=arr[i];
        j := i;
     

        WHILE (J>=0 and array [j]>current) DO
        arr[j+1] :=arr [j];
        j=j-1;
        END_WHILE
        arr [j+1] :=current;
    END_FOR

END